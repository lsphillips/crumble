{
    "name" : "crumble",

    "version" : "3.0.1",

    "type" : "module",

    "description" : "A RFC-6265 compliant library that makes reading and writing cookies easy.",

    "author" :
    {
        "name" : "Luke Phillips",
        "email" : "lsphillips.mail@gmail.com"
    },

    "keywords" :
    [
        "cookies"
    ],

    "homepage" : "https://github.com/lsphillips/crumble",

    "bugs" :
    {
        "url" : "https://github.com/lsphillips/crumble/issues"
    },

    "devDependencies" :
    {
        "rollup" : "4.40.0",
        "@rollup/plugin-terser" : "0.4.4",
        "@rollup/plugin-babel" : "6.0.4",
        "eslint" : "9.25.1",
        "eslint-config-protect-me-from-my-stupidity" : "15.0.0",
        "@babel/core" : "7.26.10",
        "@babel/preset-env" : "7.26.9"
    },

    "main" : "crumble.cjs",

    "exports" :
    {
        "import" : "./crumble.js",
        "require" : "./crumble.cjs",
        "default" : "./crumble.cjs"
    },

    "files" :
    [
        "/README.md",
        "/CHANGELOG.md",
        "/LICENSE.txt",
        "/crumble.js",
        "/crumble.cjs",
        "/crumble.d.ts"
    ],

    "repository" :
    {
        "type" : "git",
        "url" : "https://github.com/lsphillips/crumble.git"
    },

    "license" : "MIT",

    "scripts" :
    {
        "test:lint" : "eslint \"**/*.js\"",
        "test:unit" : "node --test",
        "test" : "npm run test:lint && npm run test:unit",
        "build" : "rollup --config",
        "prepack" : "npm run test && npm run build"
    },

    "types" : "crumble.d.ts"
}
